{
  "name": "frontend_new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.12.1"
  },
  "scripts": {
    "compile": "formatjs compile-folder ./lang ./compiled-lang",
    "dev": "vite",
    "extract": "formatjs extract 'src/**/*.{ts,tsx}' --out-file ./lang/lang.json",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "lint": "eslint src/ --ext .tsx,.ts",
    "lint-fix": "eslint src/ --ext .tsx,.ts --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check-types": "tsc --noemit"
  },
  "dependencies": {
    "bootstrap": "5.3.0",
    "phoenix": "1.7.11",
    "qr-code-styling": "1.6.0-rc.1",
    "react": "^18.2.0",
    "react-bootstrap": "2.10.2",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-intl": "6.4.4",
    "react-responsive-pagination": "^2.2.4",
    "react-router-dom": "6.14.1",
    "react-wordcloud": "https://github.com/b310-digital/react-wordcloud.git#ce1ee11d15cb39693414847c2e83e8199258b788"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@formatjs/cli": "^5.1.12",
    "@formatjs/ts-transformer": "^3.11.5",
    "@testing-library/react": "^13.4.0",
    "@types/phoenix": "1.5.4",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.23.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-formatjs": "^4.3.9",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest-clipboard": "0.0.11",
    "jsdom": "24.0.0",
    "sass": "1.74.1",
    "typescript": "5.1.6",
    "vite": "5.2.8",
    "vitest": "0.33.0"
  }
}
