{
  "name": "frontend_new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "compile": "formatjs compile-folder ./lang ./compiled-lang",
    "dev": "vite",
    "extract": "formatjs extract 'src/**/*.{ts,tsx}' --out-file ./lang/lang.json",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint-fix": "eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check-types": "tsc --noemit"
  },
  "dependencies": {
    "bootstrap": "5.3.7",
    "phoenix": "1.7.21",
    "qr-code-styling": "1.9.1",
    "react": "^18.2.0",
    "react-bootstrap": "2.10.10",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-intl": "6.6.8",
    "react-responsive-pagination": "^2.6.0",
    "react-router-dom": "6.14.1",
    "react-wordcloud": "https://github.com/b310-digital/react-wordcloud.git#ce1ee11d15cb39693414847c2e83e8199258b788"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.18.6",
    "@eslint/js": "^9.19.0",
    "@formatjs/cli": "^6.7.1",
    "@formatjs/ts-transformer": "^3.14.0",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@testing-library/react": "^16.3.0",
    "@types/phoenix": "1.6.6",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^8.57.1",
    "eslint-plugin-formatjs": "^4.3.9",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-testing-library": "^7.2.1",
    "jest-clipboard": "0.0.11",
    "jsdom": "24.1.0",
    "sass": "1.89.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "7.1.12",
    "vitest": "3.2.4"
  }
}
